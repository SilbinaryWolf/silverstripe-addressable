{
    "name": "symbiote/silverstripe-addressable",
    "description": "SilverStripe addressable and geocoding module",
    "type": "silverstripe-vendormodule",
    "keywords": [
        "silverstripe",
        "symbiote",
        "addressable",
        "geocoding"
    ],
    "license": "BSD-3-Clause",
    "authors": [
        {
            "name": "Symbiote",
            "homepage": "https://www.symbiote.com.au/"
        },
        {
            "name": "Marcus Nyeholt",
            "email": "nyeholt@symbiote.com.au"
        }
    ],
    "require": {
        "silverstripe/framework": "^4.0"
    },
    "scripts": {
        "phpcbf": "phpcbf src/ tests/",
        "phpcs": "phpcs src/ tests/"
    },
    "autoload": {
        "psr-4": {
            "Symbiote\\Addressable\\": "src/",
            "Symbiote\\Addressable\\Tests\\": "tests/"
        }
    },
    "replace": {
        "ajshort/silverstripe-addressable": "self.version",
        "silverstripe-australia/addressable": "self.version"
    }
}
